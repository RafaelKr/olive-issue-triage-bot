{
  "name": "stale-action",
  "version": "1.0.0",
  "private": true,
  "description": "Marks old issues and PRs as stale",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest",
    "dist": "npm run build && ncc build --source-map lib/index.js -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/stale.git"
  },
  "keywords": [
    "actions",
    "node",
    "stale"
  ],
  "author": "GitHub",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.1",
    "@actions/github": "^2.0.1",
    "@octokit/rest": "^16.36.0",
    "matcher": "^2.1.0",
    "module-alias": "^2.2.2",
    "semver": "^6.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.6.8",
    "@types/semver": "^6.0.0",
    "@zeit/ncc": "^0.21.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-circus": "^24.7.1",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && npm run format && npm run dist && git add dist/*"
    }
  }
}
